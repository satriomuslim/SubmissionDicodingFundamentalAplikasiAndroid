<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.submissionkeduabaru">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SubmissionKeduaBaru">
        <activity
            android:name=".main.splash.SplashScreen"
            android:theme="@style/SplashScreen"
            android:exported="true">
            <intent-filter>
                 <action android:name="android.intent.action.MAIN" />

                 <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main.mode.ModeActivity"
            android:exported="false" />
        <activity
            android:name=".main.main.FavoriteActivity"
            android:exported="false" />
        <activity
            android:name=".main.detail.DetailUserActivity"
            android:exported="false" />
        <activity
            android:name=".main.main.MainActivity" >
        </activity>
    </application>

</manifest>